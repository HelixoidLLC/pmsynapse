{
  "name": "@pmsynapse/rpc",
  "version": "0.1.0",
  "description": "Shared RPC types and utilities for PMSynapse",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linting configured'",
    "test": "echo 'No tests configured'",
    "clean": "rm -rf dist",
    "release": "pnpm build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
